{
  "name": "jiva-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate-map": "npx safetest generate-import-map src/app/Bootstrap.tsx src > src/app/imports.tsx",
    "safetest": "cross-env OPT_URL=http://localhost:3000/ vitest --config vite.safetest.config.mts",
    "safetest:jest": "cross-env OPT_URL=${OPT_URL:-http://localhost:3000/} jest --runInBand --testMatch '**/*.safetest.{j,t}s{,x}' --setupFilesAfterEnv ./setup-safetest.ts",
    "safetest:ci:test": "rm -f artifacts.json && OPT_URL=https://safetest-two.vercel.app/ OPT_CI=1 OPT_DOCKER=1 OPT_ARTIFACTS=artifacts.json npm run safetest -- --run --bail=5",
    "safetest:ci": "(npm run safetest:ci:test || true)",
    "process:ci": "npx safetest add-artifact-info artifacts.json results.json && cp results.json ../../build/results.json",
    "test": "jest"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.1.5",
    "@aws-sdk/client-connect": "^3.588.0",
    "@aws-sdk/client-s3": "^3.583.0",
    "@aws-sdk/s3-request-presigner": "^3.583.0",
    "@emotion/react": "^11.11.4",
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.1.3",
    "@magidoc/cli": "^4.1.4",
    "@nextui-org/react": "^2.2.10",
    "aws-amplify": "^6.0.20",
    "bootstrap": "^5.3.3",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.28",
    "lucide-react": "^0.378.0",
    "next": "14.1.1",
    "rc-progress": "^4.0.0",
    "react": "^18",
    "react-beautiful-dnd": "^13.1.1",
    "react-bootstrap": "^2.10.2",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^18",
    "react-h5-audio-player": "^3.9.1",
    "react-loading": "^2.0.3",
    "react-native": "^0.73.6",
    "react-native-chart-kit": "^6.12.0",
    "simplex-noise": "^4.0.1",
    "tailwind-merge": "^2.2.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18.2.78",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.1.1",
    "jest": "^29.7.0",
    "postcss": "^8",
    "safetest": "^0.1.13",
    "tailwindcss": "^3.4.3",
    "typescript": "^5",
    "vitest": "^1.0.4"
  }
}
